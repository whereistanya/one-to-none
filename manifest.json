{
"name":"One To None",
"description":"Forgetting to add the other person in your two person meeting",
"version":"1",
"manifest_version":2,
"content_scripts": [
    {
      "matches": ["*://calendar.google.com/*"],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ],
"background": {
  "scripts": ["background.js"],
  "persistent": false
  },
"permissions": ["identity", "identity.email"]
}
